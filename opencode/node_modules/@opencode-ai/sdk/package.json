{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@opencode-ai/sdk",
  "version": "0.15.14",
  "type": "module",
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "build": "./script/build.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
