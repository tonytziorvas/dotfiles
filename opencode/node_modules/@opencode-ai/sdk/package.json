  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@hey-api/openapi-ts": "0.81.0",
    "@tsconfig/node22": "22.0.2",
    "@types/node": "22.13.9",
    "typescript": "5.8.2",
    "@typescript/native-preview": "7.0.0-dev.20251014.1"
  },
  "dependencies": {},
